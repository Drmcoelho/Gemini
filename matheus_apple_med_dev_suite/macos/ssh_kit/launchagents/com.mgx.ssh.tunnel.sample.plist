<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple Computer//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
  <key>Label</key><string>com.mgx.ssh.tunnel.sample</string>
  <key>ProgramArguments</key>
  <array>
    <string>/bin/bash</string>
    <string>-lc</string>
    <string>PATH=/opt/homebrew/bin:/usr/local/bin:/usr/bin:/bin
HOST=${HOST} LPORT=${LPORT} RHOST=${RHOST} RPORT=${RPORT} IDENT=${IDENT} ${HOME}/.local/bin/ssh-tunnel</string>
  </array>
  <key>RunAtLoad</key><true/>
  <key>KeepAlive</key><true/>
  <key>StandardOutPath</key><string>${HOME}/Library/Logs/ssh-tunnel.sample.log</string>
  <key>StandardErrorPath</key><string>${HOME}/Library/Logs/ssh-tunnel.sample.err</string>
</dict>
</plist>
